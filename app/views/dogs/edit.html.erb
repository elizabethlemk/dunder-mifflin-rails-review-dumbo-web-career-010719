
<h1>Edit Dog:</h1>
<%= form_for @dog do |f| %>
  <div >
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div>
    <%= f.label :breed %>
    <%= f.text_field :breed %>
  </div>
  <div>
    <%= f.label :age %>
    <%= f.text_field :age %>
  </div>
  <%= f.submit "Submit" %>
<% end %>

<%= button_to "Back", dog_path, method: :get %>
